#include <stdio.h>

int main() {
  int num,sum=0,count=0;
  scanf("%d", &num);
  if(num==0){
      count=1;
  }
  else if(num!=0)
  {
  while(num>0)
  {
      count++;
      num/=10;
  }
  }
  
  printf("%d ", count);

    return 0;
}

Sample Input : 1
47580 (input number)
Sample Output : 1
5 (number of digits)

Sample Input : 2
-234
Sample Output : 1
3
