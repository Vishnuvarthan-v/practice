#include <stdio.h>

int main() {
    int a,temp,digit,sum=0;
    scanf("%d", &a);
    
    temp=a;
    
    while(temp>0){
        digit=temp%10;
        sum+=digit;
        temp/=10;
    }
    
    if(a%sum==0){
        printf("%d is a Harshad number.", a);
    }
    else{
        printf("%d is a not Harshad Number.", a);
    }
    
    

    return 0;
}

Sample Input : 1
18 (Input)
Sample Output : 1
18 is Harshad number. (Output)

Sample Input : 2
99 (Input)
Sample Output : 2
99 is Not Harshad number. (Output)
