#include <stdio.h>

int main() {
  int a;
  scanf("%d", &a);
  
  if(a%2==0){
      for(int i=1;i<=a;i++){
          if(i%2==0){
              printf("%d ", i*i);
          }
          else{
              continue;
          }
      }
  }
  
  else{
      for(int i=1;i<=a;i++){
          if(i%2!=0){
              printf("%d ", i*i);
          }
          else{
              continue;
          }
      }
  }
    return 0;
}

Sample Input 1:
6 (input value)

Sample Output 1:
4 16 36

Sample Input 2:
11
